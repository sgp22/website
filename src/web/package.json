{
  "name": "docs-site",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "build:prod": "rm -rf dist/* && webpack --optimize-minimize --config config/webpack.prod.js",
    "build:dev": "rm -rf dist/* && webpack --config config/webpack.dev.js",
    "e2e:setup": "webdriver-manager update --standalone false --gecko false --quiet",
    "e2e:prod": "npm run e2e:setup && protractor ./protractor.conf.js --baseUrl=\"http://localhost:3000\"",
    "e2e:dev": "npm run e2e:setup && protractor ./protractor.conf.js",
    "lint": "npm run lint:ng && npm run lint:css && npm run lint:md",
    "lint:css": "./node_modules/stylelint-cli/bin/stylelint src/css/components/*.css; exit 0;",
    "lint:ng": "npx ng lint",
    "lint:md": "npx markdownlint ./README.md && npx markdownlint docs/**/*.md",
    "test": "npx karma start ./karma.conf.js",
    "test:once": "npx karma start ./karma.conf.js --single-run",
    "start": "npm run build:dev"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^0.4.9",
    "@angular/animations": "^5.2.11",
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/router": "^5.2.11",
    "@ngx-loading-bar/core": "1.3.1",
    "core-js": "^2.5.7",
    "ng-inline-svg": "^5.1.4",
    "node-sass": "^4.10.0",
    "rxjs": "5.5.2",
    "semver": "5.4.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/language-service": "^5.2.11",
    "@types/jasmine": "^2.8.11",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^8.10.37",
    "autoprefixer": "^7.2.6",
    "circular-dependency-plugin": "^4.4.0",
    "codelyzer": "^4.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "highlight.js": "^9.13.1",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "^2.29.0",
    "ids-css": "^1.4.0",
    "ids-identity": "^2.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "2.2.5",
    "markdownlint-cli": "^0.10.0",
    "postcss": "6.0.14",
    "postcss-at-rules-variables": "^0.1.6",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "11.0.0",
